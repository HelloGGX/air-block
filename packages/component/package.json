{
    "name": "@air-ui/component",
    "version": "0.0.0",
    "description": "air ui component library",
    "files": [
        "dist"
    ],
    "style": "dist/index.css",
    "main": "src/index.ts",
    "scripts": {
        "build": "cross-env NODE_ENV=production INPUT_DIR=src/ OUTPUT_DIR=dist/ pnpm run build:package",
        "build:package": "pnpm run build:prebuild && build:es && pnpm run build:postbuild",
        "build:prebuild": "node ./scripts/prebuild.mjs",
        "build:postbuild": "node ./scripts/postbuild.mjs",
        "build:es": "vite build --config vite.es.config.ts"
    },
    "peerDependencies": {
        "vue": "^3.2.0"
    },
    "dependencies": {
        "@element-plus/icons-vue": "^2.3.1",
        "element-plus": "^2.7.8"
    },
