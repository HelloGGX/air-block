{
    "name": "@air-ui/air-element",
    "version": "0.0.0",
    "description": "air ui component library",
    "files": [
        "dist"
    ],
    "main": "components/index.ts",
    "scripts": {
        "build": "cross-env NODE_ENV=production INPUT_DIR=components/ OUTPUT_DIR=dist/ pnpm run build:package",
        "build:package": "pnpm run build:prebuild && npm run build:es && pnpm run build:postbuild",
        "build:prebuild": "node ./scripts/prebuild.mjs",
        "build:postbuild": "node ./scripts/postbuild.mjs",
        "build:es": "vite build --config vite.es.config.ts"
    },
    "sideEffects": [
        "**/*.css",
        "dist/*",
        "es/components/*/style/*"
    ],
    "peerDependencies": {
        "vue": "^3.2.0"
    },
    "dependencies": {
        "@element-plus/icons-vue": "^2.3.1",
        "element-plus": "^2.7.8"
    },
    "devDependencies": {
        "@types/fs-extra": "^11.0.4",
        "fs-extra": "^11.2.0",
        "postcss-import": "^16.1.0",
        "sass": "^1.77.8",
        "unplugin-element-plus": "^0.8.0"
    },
    "author": "玄子",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/HelloGGX/air-ui.git"
    },
    "bugs": {},
    "engines": {
        "node": ">=20.0.0"
    }
}